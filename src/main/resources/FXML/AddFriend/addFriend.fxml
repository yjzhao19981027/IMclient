<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>

<Pane fx:id="addFriendPane" prefHeight="496.0" prefWidth="387.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller.AddFriend">
    <children>
        <Pane id="operation" layoutY="-2.0" prefHeight="26.0" prefWidth="387.0" stylesheets="@addFriend.css">
            <children>
                <Button id="group_bar_chat_min" layoutX="308.0" layoutY="-2.0" prefHeight="30.0" prefWidth="40.0" styleClass="min" />
                <Button id="group_bar_chat_close" layoutX="348.0" layoutY="-2.0" prefHeight="30.0" prefWidth="40.0" styleClass="close" />
            </children>
        </Pane>
        <Pane layoutY="24.0" prefHeight="64" prefWidth="387.0" styleClass="search" stylesheets="@addFriend.css">
            <children>
                <TextField fx:id="user_search" layoutX="16" layoutY="12.0" prefHeight="36.0" prefWidth="311.0" promptText="搜索" styleClass="searchInput">
                    <padding>
                        <Insets left="24" />
                    </padding>
                </TextField>
                <Label fx:id="user_searchAdd" layoutX="333.0" layoutY="12.0" prefHeight="36.0" prefWidth="40.0" styleClass="searchAdd" />

                        </children>
        </Pane>
        <Pane id="content" layoutY="94.0" prefHeight="402.0" prefWidth="387.0" styleClass="content">
            <children>
                <Pane id="content_pane" prefHeight="148.0" prefWidth="387.0">
                    <children>
      <!--                <Pane id="content_pane_box" layoutX="200" layoutY="36" prefWidth="680" prefHeight="448" style="-fx-background-color:transparent;-fx-border-width:0;"/>-->
                      <Label fx:id="isFriend" layoutX="12.0" layoutY="112.0" prefHeight="30.0" prefWidth="257.0" text="对方已经是您的好友" visible="false" />
                      <Button fx:id="add_friend" layoutX="304.0" layoutY="112.0" mnemonicParsing="false" onAction="#addFriendAction" prefHeight="30.0" prefWidth="74.0" text="添加好友" visible="false" />
                        <ImageView fx:id="headImg" fitHeight="60" fitWidth="60" layoutX="14.0" layoutY="14.0" />
                        <Label fx:id="userName" layoutX="74.0" layoutY="14.0" prefHeight="30.0" prefWidth="120.0" text="ID" />
                        <Label fx:id="age" layoutX="74.0" layoutY="48.0" prefHeight="30.0" prefWidth="120.0" text="18" />
                        <Label fx:id="sex" layoutX="134.0" layoutY="48.0" prefHeight="30.0" prefWidth="90.0" text="sex" />
                        <Label fx:id="motto" layoutX="74.0" layoutY="78.0" prefHeight="36.0" prefWidth="320" text="motto" />
                    </children>
                </Pane>
            </children>
        </Pane>
    </children>
</Pane>
