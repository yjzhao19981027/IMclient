<Pane fx:id="addFriendPane" prefHeight="496.0" prefWidth="405.0" stylesheets="@addFriend.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller.AddFriend">
